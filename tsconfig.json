    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src", 
    "strict": false,
    "incremental": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
